entity start
	method adicao(int x, int y) [int]
		return(x + y);
	end
	method subtracao(int x, int y) [int]
		return(x - y);
	end
	method constructor()
//		var int a, s;
		var int a;
		var int s;
		a = adicao(10,20);
		s = subtracao(10,20);
		io.writeln("Adicao dos valores    : ", a);
		io.writeln("Subtracao dos valores : ", s);
	end
end

