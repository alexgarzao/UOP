entity start
	method constructor()
		io.writeln("Fatorial de 0: ", fatorial(0));
		io.writeln("Fatorial de 1: ", fatorial(1));
		io.writeln("Fatorial de 2: ", fatorial(2));
		io.writeln("Fatorial de 3: ", fatorial(3));
		io.writeln("Fatorial de 4: ", fatorial(4));
		io.writeln("Fatorial de 5: ", fatorial(5));
		io.writeln("Fatorial de 6: ", fatorial(6));
	end
	method fatorial(int n) [int]
		if (n == 0)
			return 1;
		else
			return n*fatorial(n - 1);
		end
	end
end

