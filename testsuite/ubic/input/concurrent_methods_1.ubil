entity start
	method constructor()
		var int m1 = method1().fork();
		var int m2 = method2().fork();
		for(int x = 1; x <= 5; x++)
			writeln( "Elemento Start esta na iteracao ", x );
		end
		m1.wait();
		m2.wait();
	end
	method method1()
		for(int i = 1; x <= 10; x++)
			writeln("Running method1...");
		end
	end
	method method2()
		for(int i = 1; x <= 10; x++)
			writeln("Running method2...");
		end
	end
end

